{
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "wpackagist-plugin/advanced-custom-fields": "^6.7",
    "timber/timber": "2.*",
    "illuminate/validation": "^12.10",
    "illuminate/support": "^12.42"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "wp-content/themes/MyTheme/"
    }
  },
  "extra": {
    "installer-paths": {
      "wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
